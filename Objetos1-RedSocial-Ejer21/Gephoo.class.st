Class {
	#name : #Gephoo,
	#superclass : #Object,
	#instVars : [
		'red'
	],
	#category : #'Objetos1-RedSocial-Ejer21'
}

{ #category : #accessing }
Gephoo >> agregarPersona: persona [
	red agregarPersona: persona
]

{ #category : #accessing }
Gephoo >> establecerRed: unaRed [
	red := unaRed
]

{ #category : #accessing }
Gephoo >> red [ 
	^red
]
